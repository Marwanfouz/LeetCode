class Solution:
    def mergeAlternately(self, word1: str, word2: str) -> str:
        merge = ''
        ran = 0
        ran = len(word1) if len(word1) > len(word2) else len(word2)
        for i in range(ran):
            if i < len(word1):
                merge += word1[i]
            if i < len(word2):
                merge += word2[i]
        return merge
