class Solution:
    def closeStrings(self, word1: str, word2: str) -> bool:
        if (len(word1) != len(word2)):
            return False

        count1 = Counter(word1)
        count2 = Counter(word2)

        if (sorted(count1.values()) == sorted(count2.values()) and set(count1.keys()) == set(count2.keys())):
            return True
        return False
## Runtime 
# 122ms Beats 87.65% of users with Python3
## Memory
# 18.19 MB Beats 46.64% of users with Python3
