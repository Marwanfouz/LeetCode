class Solution:
    def findTheDifference(self, s: str, t: str) -> str:
        char_count = {}

        for char in s:
            char_count[char] = char_count.get(char, 0) + 1
        for char in t:
            char_count[char] = char_count.get(char, 0) - 1
        for char, count in char_count.items():
            if count != 0:
                return char
